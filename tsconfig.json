{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/public/*": [
        "public/*"
      ],
      "@/images/*": [
        "src/assets/images/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ]
    },
    "strictNullChecks": true
  }
}
